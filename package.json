{
  "name": "robchat-client",
  "version": "0.0.1",
  "description": "Client application for end-to-end encrypted group chat.",
  "main": "app/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "npx electron ./index.js",
    "start:build": "npm run build && npm start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx soko build --jsIn ./app/index.js --cssIn ./styles/index.scss --assetsIn ./assets"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/robmclarty/robchat-client.git"
  },
  "keywords": [
    "websocket",
    "e2e",
    "secure",
    "chat"
  ],
  "author": "Rob McLarty <r@robmclarty.com> (http://robmclarty.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robmclarty/robchat-client/issues"
  },
  "homepage": "https://github.com/robmclarty/robchat-client#readme",
  "dependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "electron": "^2.0.7",
    "soko": "^0.2.6"
  }
}
